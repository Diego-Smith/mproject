.borderNewsTitle {
	height:4px;
	width:100%;
background: -moz-radial-gradient(center, ellipse cover,  rgba(150,0,0,1) 0%, rgba(237,212,212,0) 83%, rgba(255,255,255,0) 100%); /* FF3.6+ */
background: -webkit-gradient(radial, center center, 0px, center center, 100%, color-stop(0%,rgba(150,0,0,1)), color-stop(83%,rgba(237,212,212,0)), color-stop(100%,rgba(255,255,255,0))); /* Chrome,Safari4+ */
background: -webkit-radial-gradient(center, ellipse cover,  rgba(150,0,0,1) 0%,rgba(237,212,212,0) 83%,rgba(255,255,255,0) 100%); /* Chrome10+,Safari5.1+ */
background: -o-radial-gradient(center, ellipse cover,  rgba(150,0,0,1) 0%,rgba(237,212,212,0) 83%,rgba(255,255,255,0) 100%); /* Opera 12+ */
background: -ms-radial-gradient(center, ellipse cover,  rgba(150,0,0,1) 0%,rgba(237,212,212,0) 83%,rgba(255,255,255,0) 100%); /* IE10+ */
background: radial-gradient(ellipse at center,  rgba(150,0,0,1) 0%,rgba(237,212,212,0) 83%,rgba(255,255,255,0) 100%); /* W3C */
filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#960000', endColorstr='#00ffffff',GradientType=1 ); /* IE6-9 fallback on horizontal gradient */
	}

	
