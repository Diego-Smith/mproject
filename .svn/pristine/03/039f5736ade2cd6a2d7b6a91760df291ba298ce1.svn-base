.text-shadow-with-params(@dim, @fuzziness, @red, @yel, @blue, @alpha) {
  text-shadow: 	@dim 0px @fuzziness rgba(@red, @yel, @blue,  @alpha),
  -@dim 0px @fuzziness rgba(@red, @yel, @blue,  @alpha),
  0px @dim @fuzziness rgba(@red, @yel, @blue,  @alpha),
  0px -@dim @fuzziness rgba(@red, @yel, @blue,  @alpha)
}


.text-shadow-with-params-double(@dim, @dim2, @fuzziness, @red, @yel, @blue, @alpha) {
  text-shadow: 	@dim 0px @fuzziness rgba(@red, @yel, @blue,  @alpha),
  -@dim 0px @fuzziness rgba(@red, @yel, @blue,  @alpha),
  0px @dim @fuzziness rgba(@red, @yel, @blue,  @alpha),
  0px -@dim @fuzziness rgba(@red, @yel, @blue,  @alpha),
  @dim2 0px 1px rgba(0, 0, 0, 0.3),
  -@dim2 0px 1px rgba(0, 0, 0,  0.3),
  0px @dim2 1px rgba(0, 0, 0,  0.3),
  0px -@dim2 1px rgba(0, 0, 0,  0.3);
}


